{
  "name": "betamax-core",
  "version": "1.0.0",
  "description": "BetaMax Media Player Core Lib",
  "main": "dist/index.js",
  "scripts": {
    "republish": "babel --presets es2015,es2016 --plugins 'transform-es2015-modules-umd','transform-class-properties' ./src --out-dir ./dist",
    "bundle": "webpack",
    "serve": "webpack-dev-server --content-base demo/",
    "start": "npm run serve",
    "serve-media": "http-server media -p 8081"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinegrant/betamax-core.git"
  },
  "keywords": [
    "Media",
    "Player",
    "Video",
    "Audio"
  ],
  "author": "Antoine Grant <antoine.grant@nytimes.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/antoinegrant/betamax-core/issues"
  },
  "homepage": "https://github.com/antoinegrant/betamax-core#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "hls.js": "^0.6.2-7",
    "http-server": "^0.9.0",
    "install": "^0.8.1",
    "npm": "^3.10.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webworkify-webpack": "^1.1.7"
  }
}
